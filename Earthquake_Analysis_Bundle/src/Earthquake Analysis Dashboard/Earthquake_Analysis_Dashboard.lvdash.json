{
  "datasets": [
    {
      "name": "32f56044",
      "displayName": "Earthquake Detail",
      "queryLines": [
        "SELECT\r\n",
        "    *\r\n",
        "FROM earthquake_clean.earthquake_data_final;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "eb04fe3e",
      "displayName": "Earthquake Magnitude Distribution",
      "queryLines": [
        "WITH total AS (\r\n",
        "  SELECT COUNT(*) AS total_events\r\n",
        "  FROM earthquake_clean.earthquake_data_final\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  CASE\r\n",
        "    WHEN mag < 2 THEN 'Micro (<2)'\r\n",
        "    WHEN mag BETWEEN 2 AND 3 THEN 'Minor (2–3)'\r\n",
        "    WHEN mag BETWEEN 3 AND 4 THEN 'Light (3–4)'\r\n",
        "    ELSE 'Moderate+ (4+)'\r\n",
        "  END AS magnitude_bucket,\r\n",
        "  COUNT(*) AS event_count,\r\n",
        "  ROUND(100.0 * COUNT(*) / t.total_events, 2) AS percentage\r\n",
        "FROM earthquake_clean.earthquake_data_final, total t\r\n",
        "GROUP BY magnitude_bucket, t.total_events\r\n",
        "ORDER BY\r\n",
        "  CASE magnitude_bucket\r\n",
        "    WHEN 'Micro (<2)' THEN 1\r\n",
        "    WHEN 'Minor (2–3)' THEN 2\r\n",
        "    WHEN 'Light (3–4)' THEN 3\r\n",
        "    ELSE 4\r\n",
        "  END;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "fe1d595c",
      "displayName": "Earthquake by Country",
      "queryLines": [
        "SELECT\r\n",
        "    COUNT(*) AS event_count,\r\n",
        "    CASE\r\n",
        "        WHEN SIZE(SPLIT(place, ', ')) > 1 THEN SPLIT(place, ', ')[1]\r\n",
        "        ELSE SPLIT(place, ', ')[0]\r\n",
        "    END AS country\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "GROUP BY\r\n",
        "    CASE\r\n",
        "        WHEN SIZE(SPLIT(place, ', ')) > 1 THEN SPLIT(place, ', ')[1]\r\n",
        "        ELSE SPLIT(place, ', ')[0]\r\n",
        "    END\r\n",
        "ORDER BY event_count DESC;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "a89c8db4",
      "displayName": "Earthquakes over Time",
      "queryLines": [
        "SELECT\r\n",
        "    DATE(time) AS event_date,\r\n",
        "    COUNT(*) AS total_events\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "GROUP BY DATE(time)\r\n",
        "ORDER BY event_date;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "9330025a",
      "displayName": "Top 10 Most Affected Countries",
      "queryLines": [
        "SELECT\r\n",
        "    DATE(time) AS event_date,\r\n",
        "    COUNT(*) AS total_events\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "GROUP BY DATE(time)\r\n",
        "ORDER BY event_date\r\n",
        "LIMIT 10;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "01ba1c2b",
      "displayName": "Average Magnitude by Country",
      "queryLines": [
        "SELECT\r\n",
        "    CASE\r\n",
        "        WHEN SIZE(SPLIT(place, ', ')) > 1 THEN SPLIT(place, ', ')[1]\r\n",
        "        ELSE SPLIT(place, ', ')[0]\r\n",
        "    END AS country,\r\n",
        "    ROUND(AVG(mag), 2) AS avg_magnitude\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "GROUP BY country\r\n",
        "ORDER BY avg_magnitude DESC;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "ad3908cb",
      "displayName": "Tsunami vs Non-Tsunami Events",
      "queryLines": [
        "SELECT\r\n",
        "    CASE\r\n",
        "        WHEN tsunami = 1 THEN 'Tsunami'\r\n",
        "        ELSE 'No Tsunami'\r\n",
        "    END AS tsunami_flag,\r\n",
        "    COUNT(*) AS total_events\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "GROUP BY tsunami_flag;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "99db5890",
      "displayName": "Depth vs Magnitude Relationship",
      "queryLines": [
        "SELECT\r\n",
        "    depth,\r\n",
        "    mag\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "WHERE depth IS NOT NULL\r\n",
        "  AND mag IS NOT NULL\r\n",
        "ORDER BY depth DESC;"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "fb086e29",
      "displayName": "Strong Earthquake Events (Magnitude ≥ 5)",
      "queryLines": [
        "SELECT\r\n",
        "    COUNT(*) AS strong_earthquakes\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "WHERE mag >= 5;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "2d8a926f",
      "displayName": "Most Recent Major Earthquakes",
      "queryLines": [
        "SELECT\r\n",
        "    time,\r\n",
        "    place,\r\n",
        "    mag,\r\n",
        "    depth\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "WHERE mag >= 5\r\n",
        "ORDER BY time DESC\r\n",
        "LIMIT 10;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    },
    {
      "name": "2090746b",
      "displayName": "Tsunami Events",
      "queryLines": [
        "SELECT time, COUNT(*) AS tsunami_events\r\n",
        "FROM earthquake_clean.earthquake_data_final\r\n",
        "WHERE tsunami = 1\r\n",
        "GROUP BY time;\r\n"
      ],
      "catalog": "earthquake_dev",
      "schema": "default"
    }
  ],
